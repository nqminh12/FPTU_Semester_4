<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>HelloWorld</servlet-name>
        <servlet-class>controller.HelloWorld</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>controller.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>k</servlet-name>
        <servlet-class>controller.k</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Draw</servlet-name>
        <servlet-class>controller.Draw</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadDB</servlet-name>
        <servlet-class>controller.LoadDB</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HelloWorld</servlet-name>
        <url-pattern>/HelloWorld</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>k</servlet-name>
        <url-pattern>/k</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Draw</servlet-name>
        <url-pattern>/Draw</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadDB</servlet-name>
        <url-pattern>/LoadDB</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
